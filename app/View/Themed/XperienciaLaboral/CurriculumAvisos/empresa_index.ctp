<?php 	echo $this->Html->css('bootstrap-slider');	echo $this->Html->script('bootstrap-slider');	echo $this->Html->script('jQuery.print');?><script>	var CVS = {};		var conditions = {		<?php foreach($this->params->params['named'] as $index => $named){ ?>			<?php echo str_replace('.','__',$index).': "'.$named.'",'; ?>		<?php } ?>	};		$(document).ready(function(){		$('a.favorite').click(function(){			ToggleFav($(this).attr('id'));		});				$('#ExtraPaisId').change(function(){			UpdateCombo($('#DatosPersonalesProvinciaResidenciaId'),'<?php echo $this->Html->Url(array('controller'=>'datos_personales','action'=>'get_provincias')); ?>/'+$(this).val());		});				$('#listado-postulantes .sort').click(function(){			Sort($(this));		});				$('#btn-submit').click(function(){			Filtrar();		});				$('#btn-clean').click(function(){			LimpiarFiltros();		});				$('#myTab a').click(function (e) {		  e.preventDefault()		  $(this).tab('show')		});				$('#btn-filtros').click(function(){			$('#filtros-container').modal('show');		});				$('#filtros').trigger("reset");				$("#edad_interval").slider({});				$("#sueldo_minimo").slider({});				/* Tooltips */		$('#botonera-listado button').tooltip({container: 'body'});		$('#listado-postulantes table .acciones a').tooltip();		$('.tooltip-link').tooltip();	});		function GetListado(){		var conditions_encoded = '';		$.each(conditions,function(index,value){			conditions_encoded += '/'+index.replace(/__/g,'.')+':'+value;		});		$.post('<?php echo $this->Html->Url(array('controller'=>'curriculum_avisos','action'=>'index',$aviso['Aviso']['id'])); ?>'+conditions_encoded,function(response){			if(response != 'error'){				$('#listado-postulantes tbody').html('');				$('#listado-postulantes tbody').append(response);			}		});		GetPages();		setTimeout(function(){$('#listado-postulantes table .acciones a').tooltip();},1000);	}		function GetPages(){		var conditions_encoded = '';		$.each(conditions,function(index,value){			conditions_encoded += '/'+index.replace('__','.')+':'+value;		});		$.post('<?php echo $this->Html->Url(array('controller'=>'curriculum_avisos','action'=>'index',$aviso['Aviso']['id'])); ?>/true'+conditions_encoded,function(response){			if(response != 'error'){				var response_decoded = JSON.parse(response);				if(response_decoded.pages > 1){					$('#pages').html('');					for(var i=1; i<= response_decoded.pages; i++){						if(i == response_decoded.current_page){							$('#pages').append('<button type="button" class="btn btn-default active">'+i+'</button>');						}else{							$('#pages').append('<button type="button" onClick="ChangePage('+i+');" class="btn btn-default">'+i+'</button>');						}					}				}				if(response_decoded.count == 1){					$('#n-resultados').text('Se encontró 1 postulante');				}else{					$('#n-resultados').text('Se encontraron '+response_decoded.count+' postulantes');				}			}		});	}		function Sort(element){		if(!element.hasClass('asc')){			conditions['sort'] = element.attr('id').replace('sort-','');			conditions['direction'] = 'asc';			$('#listado-postulantes .sort').removeClass('desc');			$('#listado-postulantes .sort').removeClass('asc');			element.addClass('asc');			$('#listado-postulantes .sort .glyphicon').remove();			element.append('<span style="margin-left: 5px;" class="glyphicon glyphicon-chevron-up"></span>');		}else{			conditions['sort'] = element.attr('id').replace('sort-','');			conditions['direction'] = 'desc';			$('#listado-postulantes .sort').removeClass('desc');			$('#listado-postulantes .sort').removeClass('asc');			element.addClass('desc');			$('#listado-postulantes .sort .glyphicon').remove();			element.append('<span style="margin-left: 5px;" class="glyphicon glyphicon-chevron-down"></span>');		}		GetListado();	}		function Filtrar(){		$('#filtros input, #filtros select').each(function(){			var name = $(this).attr('name').replace('data[','').replace(']','').replace('[]','');			if($(this).is(':checkbox')){				if($(this).prop('checked')){					conditions[name] = $(this).val();				}			}else{				if(name != undefined && $(this).val() != '' && $(this).val() != null && $(this).attr('type') != 'hidden'){					conditions[name] = $(this).val();				}			}		});		$('#filtros-container').modal('hide');		$('#botonera-listado #btn-clean').show();		GetListado();	}		function TogglePreferidos(state){		LimpiarFiltros(false);		conditions['CurriculumAviso__preferido'] = state;		GetListado();	}		function ToggleNuevos(state){		LimpiarFiltros(false);		conditions['CurriculumAviso__nuevo'] = state;		GetListado();	}		function LimpiarFiltros(refresh){		$.each(conditions,function(index,value){			if(!(['page','sort','direction'].indexOf(index) > -1)){				delete conditions[index];			}		});		$('#filtros').trigger("reset");		$('#filtros-container').modal('hide');		$('#botonera-listado #btn-clean').hide();		if(refresh != false){			GetListado();		}	}		function UpdateCombo(element,url){		$.post(url,function(response){			data = JSON.parse(response);			element.html('<option value="">Seleccione</option>');			$.each(data,function(index,value){				element.append('<option value="'+index+'">'+value+'</option>');			});		});	}		function ToggleFav(id){		$.post('<?php echo $this->Html->Url(array('controller'=>'curriculum_avisos','action'=>'postulante_favorito')); ?>/'+id,function(response){			if(response != 'error'){				if($('#'+id).hasClass('glyphicon-star-empty')){					$('#'+id).removeClass('glyphicon-star-empty');					$('#'+id).addClass('glyphicon-star');					$('#cv-window .favorite-star').removeClass('glyphicon-star-empty');					$('#cv-window .favorite-star').addClass('glyphicon-star');				}else{					$('#'+id).addClass('glyphicon-star-empty');					$('#'+id).removeClass('glyphicon-star');					$('#cv-window .favorite-star').addClass('glyphicon-star-empty');					$('#cv-window .favorite-star').removeClass('glyphicon-star');				}			}		});	}		function ToogleSidebar(){		var width = $('#cv-sidebar').outerWidth();		if(!$('#cv-sidebar').hasClass('open')){			$('#listado-postulantes').animate({right: width},250);			$('#cv-sidebar').animate({right: '0'},250);			$('#cv-sidebar').addClass('open');		}else{			$('#listado-postulantes').animate({right: '0'},250);			$('#cv-sidebar').animate({right: -width},250);			$('#cv-sidebar').removeClass('open');		}	}		function ResizeListado(){		var width = $('#cv-sidebar').outerWidth();		$('#listado-postulantes').animate({right: width},250);	}		var cv_abierto_user_id;		function OpenCV(user_id, new_window){		if(!(user_id in CVS)){			$.post('<?php echo $this->Html->Url(array('controller'=>'usuarios','action'=>'ver')); ?>',{user_id: user_id},function(response){				if(response != 'error'){					CVS[user_id] = JSON.parse(response);					AddToSidebar(user_id);					FlagNuevo(user_id, 0);					if(!new_window){						ShowCV(user_id);					}				}			});		}else{			if(!new_window){				ShowCV(user_id);			}		}	}		function FlagNuevo(user_id, state){		var ca_id = $('[user_id="'+user_id+'"]').attr('ca_id');		$.post('<?php echo $this->Html->Url(array('controller'=>'curriculum_avisos','action'=>'marcar_leido')); ?>/'+ca_id+'/'+state,function(response){			if(response != 'error'){				if(state == 0){					$('[user_id="'+user_id+'"]').removeClass('nuevo');				}else{					$('[user_id="'+user_id+'"]').addClass('nuevo');				}			}		});	}		function AddToSidebar(user_id){		$('#cv-sidebar .items').append('<div class="sidebar-cv" id="sidebar-cv-'+user_id+'" onClick="ShowCV('+user_id+')"><div class="image"></div><div class="nombre">'+CVS[user_id].DatosPersonales.nombre+' '+CVS[user_id].DatosPersonales.apellido+'</div><a href="javascript:void(0);" class="glyphicon glyphicon-remove" onClick="RemoveCV('+user_id+')"></a></div>');				GetProfileThumbnail(user_id,35,35,function(img_url){			$('#sidebar-cv-'+user_id).find('.image').css('background-image','url(<?php echo Router::url('/'); ?>'+img_url+')');		});				if(!$('#cv-sidebar').hasClass('open')){ToogleSidebar();}		ResizeListado();	}		function RemoveCV(user_id){		if($('#cv-sidebar .items #sidebar-cv-'+user_id).hasClass('selected')){			HideCV();		}		$('#cv-sidebar .items #sidebar-cv-'+user_id).remove();		delete CVS[user_id]; 		if($('#cv-sidebar .items .sidebar-cv').length < 2){			ToogleSidebar();		}else{			ResizeListado();		}	}		function ShowCV(user_id){		if(!$('#cv-sidebar .items #sidebar-cv-'+user_id).hasClass('selected') && (user_id in CVS)){			if($('#cv-window').html() != undefined){				$('#cv-window').animate({left: '100%'},500,function(){					$('#cv-window').remove();					ShowCV(user_id);				});			}else{				$('#cv-sidebar .items .sidebar-cv').removeClass('selected');				$('#cv-sidebar .items #sidebar-cv-'+user_id).addClass('selected');				$('#listado-postulantes').append('<div id="cv-window">'+$('#cv-container-structure').html()+'</div>');				FillData(CVS[user_id]);				GetProfileThumbnail(user_id,110,110,function(img_url){					$('#cv-window .user-image').attr('src','<?php echo Router::url('/'); ?>'+img_url);				});				$('#cv-window').css('left','100%');				$('#cv-window').show();				$('#cv-window').animate({left: '0'},500);				GetNotes($('[user_id="'+user_id+'"]').attr('ca_id'));				cv_abierto_user_id = user_id;				$('#cv-window .favorite-star').attr('onClick',"ToggleFav("+$('[user_id="'+cv_abierto_user_id+'"]').attr('ca_id')+");");				if($('[user_id="'+cv_abierto_user_id+'"] .favorite').hasClass('glyphicon-star-empty')){					$('#cv-window .favorite-star').removeClass('glyphicon-star');					$('#cv-window .favorite-star').addClass('glyphicon-star-empty');				}else{					$('#cv-window .favorite-star').addClass('glyphicon-star');					$('#cv-window .favorite-star').removeClass('glyphicon-star-empty');				}			}		}	}		function HideCV(){		$('#cv-window').animate({left: '100%'},500,function(){			$('#cv-sidebar .items div').removeClass('selected');			$('#cv-window').remove();			$('#volver-listado').addClass('selected');		});	}		function GetProfileThumbnail(user_id,width,height,responsefn){		var data = {			user_id: user_id,			width: width,			height: height		}		$.post('<?php echo $this->Html->Url(array('controller'=>'usuarios','action'=>'get_profile_pic_thumb','empresas'=>false)); ?>', data, function(response){			if(response){				responsefn(response);			} 		});	}	function FillData(data,index_string){		if(index_string==undefined){index_string='';}		$.each(data,function(index,value){			if(value instanceof Object){				if(typeof index== "number"){					var model = index_string.replace('-','');					AddNewItem(model,index);				}				FillData(value,index_string+index+'-');			}else{				$('#cv-window #'+index_string+index).html(value);			}		});	}	function AddNewItem(model,index){		var template = $('#cv-window [model="'+model+'"] .template-display').html();		if(template != undefined){			template = template.replace(/index/g,index);			$('#cv-window [model="'+model+'"] .items').append(template);		}	}		function ChangePage(page){		conditions['page'] = page;		GetListado();	}		function AgregarFiltro(id){		var template = $('#filtros-'+id).find('.template').html();		var item_amount = $('#filtros-'+id+' .filters-container').find('.filter-item').size();		$('#filtros-'+id+' .filters-container').append(template.replace(/<id>/g,item_amount+1));	}		function QuitarFiltros(id){		$('#filtros-'+id+' .filters-container').find('.filter-item').remove();	}		function QuitarFiltro(id){		$('#'+id).remove();	}		var cv_nota_abierta_id;		function OpenNotes(id, action){			cv_nota_abierta_id = id;				$.post('<?php echo $this->Html->Url(array('controller'=>'curriculum_avisos','action'=>'ver_notas')); ?>/'+id,function(response){			$('#notas-container #notas-containerLabel').text('Notas sobre '+$('#cv-preview-'+id+' #nombre_posultante').text());			$('#notas-container').modal('show');			$('#nota').val('');			if(response != 'error'){				var response_decoded = JSON.parse(response);				$('#notas-container #ver').html('');				$.each(response_decoded,function(index,value){					$('#notas-container #ver').append('<div class="nota" id="nota-'+value.CurriculumavisoNota.id+'"><div class="texto">'+value.CurriculumavisoNota.nota+'</div><div class="usuario_created">'+value.UsuarioInterno.username+' | '+value.CurriculumavisoNota.created+'</div><a href="javascript:void(0);" onClick="DeleteNota('+value.CurriculumavisoNota.id+')">Borrar</a></div>');				});				if(action == 'agregar'){					$('#notas-container a[href="#agregar"]').tab('show');				}else{					$('#notas-container a[href="#ver"]').tab('show');				}			}else{				$('#notas-container a[href="#agregar"]').tab('show');			}		});	}		function OpenAddNote(){		var ca_id = $('[user_id="'+cv_abierto_user_id+'"]').attr('ca_id');		OpenNotes(ca_id,'agregar');	}			function SaveNota(){		if($('#nota').val() != ''){			var data = {				nota: $('#nota').val(),				curriculumaviso_id: cv_nota_abierta_id			};			$.post('<?php echo $this->Html->Url(array('controller'=>'curriculum_avisos','action'=>'agregar_nota')); ?>',data,function(response){				if(response != 'error'){					OpenNotes(cv_nota_abierta_id);					GetNotes(cv_nota_abierta_id);				}			});		}	}		function DeleteNota(id){		$.post('<?php echo $this->Html->Url(array('controller'=>'curriculum_avisos','action'=>'borrar_nota')); ?>/'+id,{},function(response){			if(response != 'error'){				$('#nota-'+id).remove();			}		});	}		function GetNotes(id){		$.post('<?php echo $this->Html->Url(array('controller'=>'curriculum_avisos','action'=>'ver_notas')); ?>/'+id,function(response){			if(response != 'error'){				var response_decoded = JSON.parse(response);				$('#cv-window .notas-container .notas').html('');				$.each(response_decoded,function(index,value){					$('#cv-window .notas-container .notas').append('<div class="nota"><div class="texto">'+value.CurriculumavisoNota.nota+'</div><div class="usuario_created">'+value.UsuarioInterno.username+' | '+value.CurriculumavisoNota.created+'</div></div>');				});			}		});	}</script><div class="col-md-12" id="listado-postulantes">	<h1 style="float: left;">Postulantes a <?php echo $aviso['Aviso']['puesto']; ?></h1>		<div id="botonera-listado" style="float: right; margin: 10px 0px;" class="btn-group botonera">		<button type="button" data-toggle="tooltip" data-placement="bottom" title="Todos" onClick="LimpiarFiltros();" class="btn btn-default glyphicon glyphicon-asterisk"></button>		<button type="button" data-toggle="tooltip" data-placement="bottom" title="No Preferidos" onClick="TogglePreferidos(0);" class="btn btn-default glyphicon glyphicon-star-empty"></button>		<button type="button" data-toggle="tooltip" data-placement="bottom" title="Preferidos" onClick="TogglePreferidos(1);" class="btn btn-default glyphicon glyphicon-star"></button>		<button type="button" data-toggle="tooltip" data-placement="bottom" title="Nuevos" onClick="ToggleNuevos(1);" class="btn btn-default glyphicon glyphicon-eye-open"></button>		<button type="button" data-toggle="tooltip" data-placement="bottom" title="Leidos" onClick="ToggleNuevos(0);" class="btn btn-default glyphicon glyphicon-eye-close"></button>		<button type="button" data-toggle="tooltip" data-placement="bottom" title="Filtro avanzado" id="btn-filtros" class="btn btn-primary glyphicon glyphicon-search"></button>	</div>		<br><br><br>	<h5 id="n-resultados"><?php echo 'Se encontr'.($pages['count']!=1?'aron':'ó').' '.$pages['count'].' postulante'.($pages['count']!=1?'s':''); ?></h5>		<?php if(count($postulantes)>0){ ?>		<table class="table table-hover">					<thead>				<tr>					<th><?php echo $this->Html->Link('','javascript:void(0);',array('class'=>'sort glyphicon glyphicon-star','id'=>'sort-preferido')); ?></th>					<th><?php echo